{
  "name": "@miso.ai/client-sdk-examples-algolia-all-in-one",
  "version": "1.0.0",
  "description": "Live Demo: Miso SDK - Algolia plugin",
  "keywords": [
    "Miso AI",
    "personalization",
    "Algolia"
  ],
  "scripts": {
    "build:sass": "sass scss:dist/css",
    "watch:sass": "sass --watch scss:dist/css",
    "build:js": "rollup -c",
    "watch:js": "rollup -wc",
    "build": "npm run build:js && npm run build:sass",
    "serve": "node server.mjs",
    "start": "npm run build && npm run serve",
    "dev": "npm-run-all build:sass --parallel watch:*"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "dotenv": "^16.0.0",
    "koa": "^2.13.4",
    "koa-static": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.79.0",
    "rollup-plugin-dev": "^2.0.4",
    "rollup-plugin-livereload": "^2.0.5",
    "sass": "^1.54.8"
  },
  "dependencies": {
    "@algolia/autocomplete-js": "1.6.3",
    "@miso.ai/client-sdk": "1.3.1",
    "@miso.ai/client-sdk-algolia": "1.3.1",
    "instantsearch.js": "4.40.5"
  }
}