{
  "name": "@miso.ai/client-sdk-algolia",
  "description": "Algolia plugin for Miso client SDK",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "src/index.js",
  "exports": {
    "import": "./src/index.js",
    "require": "./dist/cjs/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "npx uvu test",
    "build:clean": "npx rimraf dist",
    "build:cjs": "BABEL_ENV=cjs npx babel src --root-mode upward --out-dir dist/cjs",
    "build": "npm run build:clean && npm run build:cjs"
  },
  "repository": "MisoAI/miso-client-js-sdk",
  "homepage": "https://misoai.github.io/miso-client-js-sdk/plugins/algolia",
  "license": "MIT",
  "contributors": [
    "simonpai <simon.pai@askmiso.com>"
  ],
  "bugs": {
    "url": "https://github.com/MisoAI/miso-client-js-sdk/issues"
  },
  "dependencies": {
    "@miso.ai/commons": "file:packages/commons",
    "@miso.ai/client-sdk-core": "file:packages/client-sdk-core"
  }
}
