{
  "name": "@miso.ai/client-sdk-algolia",
  "description": "A set of custom elements to work with Miso client JS SDK",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "files": [
    "dist/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "npx babel src --root-mode upward --out-dir dist/es --watch",
    "build:clean": "npx rimraf dist",
    "build:esm": "npx babel src --root-mode upward --out-dir dist/es",
    "build:cjs": "BABEL_ENV=cjs npx babel src --root-mode upward --out-dir dist/cjs",
    "build": "npm run build:clean && npm run build:esm && npm run build:cjs"
  },
  "repository": "MisoAI/miso-client-js-sdk",
  "homepage": "https://misoai.github.io/miso-client-js-sdk/plugins/algolia",
  "license": "MIT",
  "contributors": [
    "simonpai <simon.pai@askmiso.com>"
  ],
  "bugs": {
    "url": "https://github.com/MisoAI/miso-client-js-sdk/issues"
  },
  "dependencies": {
    "@miso.ai/commons": "file:packages/commons"
  }
}
