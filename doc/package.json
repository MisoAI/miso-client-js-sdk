{
  "name": "@miso.ai/client-sdk-doc",
  "private": true,
  "scripts": {
    "clean": "npx rimraf dist",
    "doc:build:11ty": "npx eleventy",
    "doc:watch:11ty": "npx eleventy --serve --port=10101",
    "doc:build:sass": "npx sass scss:dist/css",
    "doc:watch:sass": "npx sass --watch scss:dist/css",
    "doc:build": "npm-run-all doc:build:*",
    "doc:start": "npm-run-all doc:build:sass --parallel doc:watch:*",
    "doc:debug": "DEBUG=* npx eleventy"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "eleventy-plugin-toc": "^1.1.5",
    "js-yaml": "^4.1.0",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.49.8"
  }
}
