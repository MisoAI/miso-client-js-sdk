{
  "name": "@miso.ai/client-sdk",
  "description": "Miso client-side JavaScript SDK",
  "main": "dist/miso.min.js",
  "files": [
    "dist/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "npx rimraf lib dist es coverage types",
    "test": "echo later",
    "build": "GIT_HASH=$(git rev-parse ${GITHUB_SHA:-HEAD}) rollup -c",
    "start": "concurrently 'rollup -w -c rollup.config.dev.js' 'nodemon mock-service/app.mjs'",
    "doc:clean": "npx rimraf doc-dist",
    "doc:build:11ty": "npx eleventy --config=doc/.eleventy.js",
    "doc:watch:11ty": "npx eleventy --config=doc/.eleventy.js --serve --port=10201",
    "doc:build:sass": "npx sass doc/scss:doc-dist/css",
    "doc:watch:sass": "npx sass --watch doc/scss:doc-dist/css",
    "doc:build": "npm-run-all doc:build:*",
    "doc:start": "npm-run-all doc:build:sass --parallel doc:watch:*",
    "doc:debug": "DEBUG=* npx eleventy --config=doc/.eleventy.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:MisoAI/miso-client-js-sdk.git"
  },
  "contributors": [
    "simonpai <simon.pai@askmiso.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MisoAI/miso-client-js-sdk/issues"
  },
  "homepage": "https://github.com/MisoAI/miso-client-js-sdk#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@koa/cors": "^3.1.0",
    "@koa/router": "^10.1.1",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "eleventy-plugin-toc": "^1.1.5",
    "eslint": "^8.6.0",
    "js-yaml": "^4.1.0",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.63.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.49.8",
    "uuid": "^8.3.2"
  }
}
