{
  "name": "@miso.ai/client-sdk-monorepo",
  "private": true,
  "workspaces": [
    "packages/*",
    "mock-service",
    "doc"
  ],
  "scripts": {
    "clean": "npm run clean --workspaces --if-present",
    "test": "npm run test --workspaces --if-present",
    "build": "npm run build --workspaces --if-present",
    "start": "concurrently 'npm run start --workspace=packages/client-sdk' 'npm run start --workspace=mock-service'",
    "doc:build": "npm run doc:build --workspace=doc",
    "doc:start": "npm run doc:start --workspace=doc"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-replace": "^3.0.1",
    "concurrently": "^7.0.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.6.0",
    "rollup": "^2.63.0",
    "rollup-plugin-filesize": "^9.1.2",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "uuid": "^8.3.2"
  }
}
