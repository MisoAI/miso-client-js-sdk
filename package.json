{
  "name": "@miso.ai/client-sdk-monorepo",
  "private": true,
  "workspaces": [
    "packages/commons",
    "packages/progressive-markdown",
    "packages/client-sdk-core",
    "packages/client-sdk-dev-tool",
    "packages/client-sdk-ui",
    "packages/client-sdk-ui-markdown",
    "packages/client-sdk-algolia",
    "packages/client-sdk-gtm",
    "packages/client-sdk-shopify",
    "packages/client-sdk",
    "demo"
  ],
  "scripts": {
    "start": "concurrently 'npm:start:*'",
    "start:client-sdk-core": "npm run start --workspace=packages/client-sdk-core",
    "start:client-sdk-dev-tool": "npm run start --workspace=packages/client-sdk-dev-tool",
    "start:client-sdk-ui": "npm run start --workspace=packages/client-sdk-ui",
    "start:client-sdk-ui-markdown": "npm run start --workspace=packages/client-sdk-ui-markdown",
    "start:client-sdk-shopify": "npm run start --workspace=packages/client-sdk-shopify",
    "start:client-sdk": "npm run start --workspace=packages/client-sdk",
    "start:demo": "npm run start --workspace=demo",
    "test": "npm run test --workspaces --if-present",
    "version": "node ./bin/version.mjs",
    "examples": "node ./bin/examples.mjs",
    "build:clean": "npm run build:clean --workspaces --if-present",
    "build:esm": "npm run build:esm --workspaces --if-present",
    "build:cjs": "npm run build:cjs --workspaces --if-present",
    "build": "npm run build --workspaces --if-present",
    "purge-cdn": "npm run purge-cdn --workspaces --if-present"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.16.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
    "@babel/plugin-transform-export-namespace-from": "^7.22.5",
    "@babel/plugin-transform-modules-commonjs": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.2",
    "@rollup/plugin-terser": "^0.3.0",
    "concurrently": "^7.0.0",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.0",
    "eslint": "^8.6.0",
    "fast-glob": "^3.2.11",
    "js-yaml": "^4.1.0",
    "rimraf": "^4.0.4",
    "rollup": "^3.10.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "rollup-plugin-serve": "^2.0.2",
    "sass": "^1.49.8",
    "uuid": "^8.3.2",
    "uvu": "^0.5.6"
  },
  "overrides": {
    "terser": "^5.15.0"
  }
}
