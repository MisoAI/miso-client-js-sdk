{
  "name": "@miso.ai/client-sdk-demo",
  "private": true,
  "scripts": {
    "clean": "npx rimraf dist",
    "build:11ty": "npx eleventy",
    "watch:11ty": "NODE_ENV=development npx eleventy --serve --port=10100",
    "build:sass": "npx sass scss:dist/css",
    "watch:sass": "npx sass --watch scss:dist/css",
    "build:js": "rollup -c",
    "watch:js": "rollup -w -c rollup.config.dev.js",
    "build": "npm-run-all build:*",
    "start": "npm-run-all build:sass --parallel watch:*",
    "debug": "DEBUG=* npx eleventy"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "js-yaml": "^4.1.0",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.49.8"
  },
  "dependencies": {
    "@miso.ai/client-sdk": "file:packages/client-sdk",
    "@miso.ai/client-sdk-algolia": "file:packages/client-sdk-algolia",
    "instantsearch.js": "^4.40.4"
  }
}
